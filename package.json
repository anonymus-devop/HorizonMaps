{
  "name": "horizonmaps",
  "version": "1.0.0",
  "description": "AI-powered navigation with Liquid Glass UI.",
  "author": "HorizonMaps Team",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:win": "vite build && electron-builder"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "axios": "^1.13.2",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "mapbox-gl": "^3.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.4",
    "electron-builder": "^24.9.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-pwa": "^1.1.0"
  },
  "build": {
    "appId": "com.horizonmaps.app",
    "productName": "HorizonMaps",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "build/favicon.ico"
    },
    "msi": {
      "oneClick": false,
      "perMachine": true,
      "runAfterFinish": true,
      "shortcutName": "HorizonMaps"
    }
  }
}